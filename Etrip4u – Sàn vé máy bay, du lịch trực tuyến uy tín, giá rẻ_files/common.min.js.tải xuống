" ");if(1==t.length)return e.toUpperCase();for(var n=t[t.length-1],r=0;r<t.length-1;r++)n+=t[r].substring(0,1);return n.toUpperCase()}function changeAlias(e){var t=e;return t=(t=(t=(t=(t=(t=(t=(t=(t=(t=(t=t.toLowerCase()).replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ằ|ắ|ặ|ẳ|ẵ/g,"a")).replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g,"e")).replace(/ì|í|ị|ỉ|ĩ/g,"i")).replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g,"o")).replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g,"u")).replace(/ỳ|ý|ỵ|ỷ|ỹ/g,"y")).replace(/đ/g,"d")).replace(/!|@|%|\^|\*|\(|\)|\+|\=|\<|\>|\?|\/|,|\.|\:|\;|\'|\"|\&|\#|\[|\]|~|\$|_|`|-|{|}|\||\\/g," ")).replace(/ + /g," ")).trim()}$(document).ready((function(){$(document).on("keydown",".control-money",(function(e){return e.key>="0"&&e.key<="9"||"ArrowLeft"==e.key||"ArrowRight"==e.key||"Delete"==e.key||"Backspace"==e.key||"Tab"==e.key})),$(document).on("change",".control-money",(function(e){var t=ConvertMoneyToNumber($(this).val());$(this).val(t.toLocaleString("en-US"))})),$(document).on("keydown",".control-search-money",(function(e){return e.key>="0"&&e.key<="9"||"ArrowLeft"==e.key||"ArrowRight"==e.key||"Delete"==e.key||"Backspace"==e.key||"Tab"==e.key})),$(document).on("keyup",".control-search-money",(function(e){var t=$(this).val();if(""!=t){var n=ConvertMoneyToNumber(t);$(this).val(n.toLocaleString("en-US"))}}))}));