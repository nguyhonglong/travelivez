function convertCurrency(e){for(var t="",n=0,r=e.length-1;r>=0;r--)2==n?(t+=e[r]+",",n=0):(t+=e[r],n++);t.lastIndexOf(",")==t.length-1&&(t=t.substring(0,t.length-1));for(var a="",o=t.length-1;o>=0;o--)a+=t[o];return a}function convertCurrencyDot(e){for(var t="",n=0,r=(e=parseInt(e).toFixed()).length-1;r>=0;r--)2==n?(t+=e[r]+".",n=0):(t+=e[r],n++);t.lastIndexOf(".")==t.length-1&&(t=t.substring(0,t.length-1));for(var a="",o=t.length-1;o>=0;o--)a+=t[o];return a}function convertCurrency(e){for(var t="",n=0,r=(e=parseInt(e).toFixed()).length-1;r>=0;r--)2==n?(t+=e[r]+",",n=0):(t+=e[r],n++);t.lastIndexOf(",")==t.length-1&&(t=t.substring(0,t.length-1));for(var a="",o=t.length-1;o>=0;o--)a+=t[o];return a}function FormatString(e){var t=e;return t=(t=(t=(t=(t=(t=(t=(t=(t=(t=(t=t.toLowerCase()).replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ắ|ằ|ắ|ặ|ẳ|ẵ/g,"a")).replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g,"e")).replace(/ì|í|ị|ỉ|ĩ/g,"i")).replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g,"o")).replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g,"u")).replace(/ỳ|ý|ỵ|ỷ|ỹ/g,"y")).replace(/đ/g,"d")).replace(/!|%|\^|\*|\(|\)|\+|\=|\<|\>|\?|\/|,|\.|\:|\;|\'| |\"|\&|\#|\[|\]|~|$|_/g,"-")).replace(/-+-/g,"-")).replace(/^\-+|\-+$/g,"")}function ShowBlock(e,t){e.block({css:{border:"none",padding:"15px",backgroundColor:"#000","-webkit-border-radius":"10px","-moz-border-radius":"10px",opacity:1,color:"#fff"},message:'<h2 style="color:#fff">'+t+" ...</h2>"})}function addDefaultOption(e,t){e.empty(),e.append($("<option></option>").attr("value","-1").text("Chọn "+t))}function addSelectListWithDefaultValue(e,t,n){0!=t.length?$.each(t,(function(t,r){var a=r.Text,o=$("<option></option>").attr("value",r.Value).text(a);null!=n&&n==r.Value&&o.attr("selected","selected"),e.append(o)})):e.html('<option value="-1">Không có dữ liệu</option>')}function addSelectListItems(e,t){if(0!=t.length)return 1==t.length?(e.append($("<option></option>").attr("value",t[0].Value).attr("selected",!0).text(t[0].Text)),void e.change()):void $.each(t,(function(t,n){e.appe